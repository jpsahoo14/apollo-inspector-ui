{
  "manifest_version": 2,
  "name": "Apollo Inspector",
  "description": "Adds apollo inspector devtools to Firefox",
  "version": "0.0.1",
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": ["webpage.js"],
  "background": {
    "scripts": ["background.js"]
  }
}
